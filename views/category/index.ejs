<!DOCTYPE html>
<html>
  <head>
    <title>Categories</title>
  </head>
  <body>
    <h1>Categories</h1>
    <a href="/categories/new">New Category</a>
    <ul>
      <% categories.forEach((category) => { %>
        <li><a href="/categories/<%= category.id %>"><%= category.name %></a></li>
      <% }) %>
    </ul>
  </body>
</html>
